{
  "expo": {
    "name": "Keeniela",
    "slug": "keeniela",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "keeniela",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gonzzdev.keeniela"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash-light.png",
        "backgroundColor": "#007BFF"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.gonzzdev.keeniela"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-light.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#007BFF",
          "dark": {
            "image": "./assets/images/splash-dark.png",
            "backgroundColor": "#00A3FF"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "4c242574-1fb8-4f8e-bac9-b1107f4942ac"
      }
    }
  }
}
